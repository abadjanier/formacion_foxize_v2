<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Backoffice\Users\Domain\User" table="users">
        <id name="id" type="integer" column="id" length="36" >
            <generator strategy="AUTO"/>
        </id>
        <field name="email" type="user_email" column="user_email"/>
        <field name="name" type="user_name" column="user_name"/>
        <field name="surname" type="user_surname" column="user_surname"/>
        <one-to-many field="curriculums" target-entity="App\Backoffice\Curriculums\Domain\Curriculum" mapped-by="user" >
        <cascade>
            <cascade-all>
            </cascade-all>
        </cascade>
        </one-to-many>
    </entity>
</doctrine-mapping>
